{
  "web_interface": {
    "title": "Real-time Voice Translation",
    "max_upload_size": 500,
    "supported_formats": ["mp4", "avi", "mov", "mkv"],
    "default_parameters": {
      "source_language": "ja",
      "target_language": "en",
      "top_k": 15,
      "top_p": 0.7,
      "temperature": 1.0,
      "speed": 1.1
    },
    "cleanup_temp_files": true,
    "temp_dir_prefix": "streamlit_translation_"
  },
  "processing": {
    "whisper_model": "small",
    "cache_results": false,
    "show_progress": true
  }
}
